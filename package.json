{
  "name": "rawgo-api",
  "version": "1.0.2",
  "type": "module",
  "private": true,
  "description": "Easily create memes or apply different filters to images",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --noEmit",
    "start": "tsx .",
    "dev": "nodemon --import tsx .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "Dan <hi@danmutombo.com>",
  "license": "LGPL-3.0-only",
  "engines": {
    "node": ">=20.18.3"
  },
  "dependencies": {
    "canvas": "^3.1.0",
    "fastify": "^5.2.1",
    "gif-encoder": "^0.7.2",
    "gif-encoder-2": "^1.0.5",
    "gif-frames": "^1.0.1",
    "jimp": "^0.22.12",
    "mongodb": "^6.13.1",
    "node-fetch": "^3.3.2",
    "ofetch": "^1.4.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@fastify/compress": "^7.0.3",
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@stylistic/eslint-plugin-ts": "^2.13.0",
    "@types/node": "^20.17.19",
    "c12": "^1.11.2",
    "consola": "^3.4.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "nodemonConfig": {
    "ext": "ts",
    "watch": [
      "src/*"
    ],
    "delay": 2500
  },
  "packageManager": "pnpm@9.15.6"
}
