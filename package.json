{
  "name": "rawgo-api",
  "version": "1.0.2",
  "type": "module",
  "private": true,
  "description": "Easily create memes or apply different filters to images",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --noEmit",
    "start": "tsx .",
    "dev": "nodemon --import tsx .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "Dan <hi@danmutombo.com>",
  "license": "LGPL-3.0-only",
  "engines": {
    "node": ">=20.19.1"
  },
  "dependencies": {
    "canvas": "^3.1.0",
    "fastify": "^5.3.2",
    "gif-encoder": "^0.7.2",
    "gif-encoder-2": "^1.0.5",
    "gif-frames": "^1.0.1",
    "jimp": "^1.6.0",
    "mongodb": "^6.16.0",
    "node-fetch": "^3.3.2",
    "ofetch": "^1.4.1",
    "sharp": "^0.34.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@fastify/compress": "^8.0.1",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.2.2",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/node": "^22.15.3",
    "c12": "^3.0.3",
    "consola": "^3.4.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "nodemonConfig": {
    "ext": "ts",
    "watch": [
      "src/*"
    ],
    "delay": 2500
  },
  "packageManager": "pnpm@9.15.9"
